{
  "name": "vue3-admin-template",
  "version": "1.0.0",
  "description": "使用 Vue 技术栈搭建的后台模板",
  "author": {
    "name": "lio",
    "email": "licroning@163.com",
    "url": "https://github.com/lio-zero"
  },
  "license": "MIT",
  "homepage": "https://github.com/lio-zero/vue3-admin-template",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lio-zero/vue3-admin-template"
  },
  "bugs": {
    "url": "https://github.com/lio-zero/vue3-admin-template/issues"
  },
  "engines": {
    "node": "^12 || >=14"
  },
  "scripts": {
    "serve": "npm run dev",
    "dev": "vite",
    "build": "vite build",
    "preview": "npm run build && vite preview",
    "preview:dist": "vite preview",
    "type:check": "vue-tsc --noEmit --skipLibCheck",
    "lint:stylelint": "stylelint --cache --fix \"**/*.{vue,postcss,css,scss}\" --cache --cache-location node_modules/.cache/stylelint/",
    "lint:lint-staged": "lint-staged -c ./.husky/lintstagedrc.js",
    "lint:eslint": "eslint --cache --max-warnings 0  \"{src,mock}/**/*.{vue,ts,tsx}\" --fix",
    "test:unit": "jest",
    "test:unit-coverage": "jest --coverage",
    "prepare": "husky install",
    "up": "taze major -I"
  },
  "dependencies": {
    "@element-plus/icons": "^0.0.11",
    "@iconify/iconify": "^3.0.0",
    "@vueuse/core": "^9.3.1",
    "axios": "^1.1.3",
    "crypto-js": "^4.1.1",
    "dayjs": "^1.11.6",
    "element-plus": "^2.2.19",
    "lodash-es": "^4.17.21",
    "mockjs": "^1.1.0",
    "nprogress": "^0.2.0",
    "path-browserify": "^1.0.1",
    "path-to-regexp": "^6.2.1",
    "pinia": "^2.0.23",
    "qs": "^6.11.0",
    "vue": "^3.2.41",
    "vue-router": "^4.1.5",
    "vue-types": "^4.2.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.27.0",
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@iconify/json": "^2.1.122",
    "@purge-icons/generated": "^0.9.0",
    "@types/node": "^18.11.3",
    "@types/nprogress": "^0.2.0",
    "@vitejs/plugin-legacy": "^2.2.0",
    "@vitejs/plugin-vue": "^3.1.2",
    "@vitejs/plugin-vue-jsx": "^2.0.1",
    "@vue/compiler-sfc": "^3.2.41",
    "autoprefixer": "^10.4.12",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "eslint": "^8.26.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "postcss": "^8.4.18",
    "postcss-html": "^1.5.0",
    "postcss-scss": "^4.0.5",
    "rollup-plugin-visualizer": "^5.8.3",
    "sass": "^1.55.0",
    "stylelint": "^14.14.0",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended": "^9.0.0",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard": "^29.0.0",
    "stylelint-order": "^5.0.0",
    "taze": "^0.8.2",
    "terser": "^5.15.1",
    "typescript": "^4.8.4",
    "unplugin-auto-import": "^0.11.2",
    "unplugin-icons": "^0.14.12",
    "unplugin-vue-components": "^0.22.8",
    "vite": "^3.1.8",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-html": "^3.2.0",
    "vite-plugin-mock": "^2.9.6",
    "vite-plugin-pages": "^0.27.0",
    "vite-plugin-purge-icons": "^0.9.1",
