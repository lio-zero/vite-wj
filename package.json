{
  "name": "vue-vzr-admin-template",
  "version": "1.0.0",
  "description": "使用 Vue 技术栈搭建的后台模板",
  "author": {
    "name": "lio",
    "email": "licroning@163.com",
    "url": "https://github.com/lio-zero"
  },
  "keywords": [
    "vue3",
    "vite",
    "admin",
    "dashboard",
    "element-plus",
    "typescript"
  ],
  "scripts": {
    "serve": "npm run dev",
    "dev": "vite",
    "build": "cross-env VITE_USE_MOCK=true NODE_ENV=production vite build",
    "preview": "npm run build && vite preview",
    "preview:dist": "vite preview",
    "type:check": "vue-tsc --noEmit --skipLibCheck",
    "prepare": "husky install",
    "prettier": "prettier --write .",
    "lint": "eslint src --fix --ext .ts,.tsx,.vue,.js,.jsx",
    "lint:fix": "eslint . --fix && npm run prettier"
  },
  "lint-staged": {
    "*.{js,jsx,vue,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
